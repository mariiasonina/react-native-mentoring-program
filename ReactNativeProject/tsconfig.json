{
	"extends": "@tsconfig/react-native/tsconfig.json",
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@src/*": ["src/*"],
			"@components/*": ["src/components/*"],
			"@screens/*": ["src/screens/*"],
			"@styles/*": ["src/styles/*"],
			"@assets/*": ["src/assets/*"]
		}
	},
	"include": [".", "src"]
}
